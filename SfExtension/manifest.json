{
  "name": "Deployment Status Helper",
  "description": "Synched or Not Synched Meta Data",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "browser_action": {
    "default_title": "Deployment Status"
  },
  "manifest_version": 2
}